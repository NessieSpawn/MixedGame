void function Modded_Melee_Synced_Human_Init()
{
	if ( IsLobby() ) // lobby won't load anything
		return

    AddCallback_EntitiesDidLoad( InitModdedHumanSyncedMelee ) // wait for syncedmelee chooser to be created
}

